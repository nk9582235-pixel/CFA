                    "choices": [
                        {
                            "id": "ae51d5a5-5fd9-42ff-910f-ee56032bc1fc",
                            "itemBody": "A. significant, because the calculated test statistic is outside the bounds of the critical values for the test statistic.",
                            "position": 1
                        },
                        {
                            "id": "ad36bff4-9c6f-4ea4-afd1-50c36f89fe73",
                            "itemBody": "B. significant, because the calculated test statistic has a lower absolute value than the critical value for the test statistic.",
                            "position": 2
                        },
                        {
                            "id": "669c41ec-469d-46bc-bd70-30fb9f032fa0",
                            "itemBody": "C. insignificant, because the calculated test statistic is outside the bounds of the critical values for the test statistic.",
                            "position": 3
