        {
            "id": "53122",
            "entry": {
                "title": "CFA2404-R-s02-Q6",
                "feedback": {
                    "neutral": "\n            \u003cp style=\"margin-top: 0.75rem; margin-bottom: 0.75rem;\"\u003eB is correct. By definition, lognormal random variables cannot have negative values (bounded below by 0) and have distributions that are skewed to the right.\u003c/p\u003e\n         "
                },
                "itemBody": "\u003cp style=\"margin-top: 0.75rem; margin-bottom: 0.75rem;\"\u003eIn contrast to normal distributions, lognormal distributions:\u003c/p\u003e",
                "properties": {
                    "shuffleRules": {
                        "choices": {
                            "toLock": [],
                            "shuffled": false
                        }
                    },
                    "varyPointsByAnswer": false
                },
                "scoringData": {
                    "value": "f49a3cc7-b554-478b-bb36-6eeb0e267232"
                },
                "answerFeedback": {},
                "calculatorType": "none",
                "interactionData": {
                    "choices": [
                        {
                            "id": "daa43af2-f34c-4653-bccd-0a8ba591e455",
                            "itemBody": "A. are skewed to the left.",
                            "position": 1
                        },
                        {
                            "id": "f49a3cc7-b554-478b-bb36-6eeb0e267232",
                            "itemBody": "B. have outcomes that cannot be negative.",
                            "position": 2
                        },
                        {
                            "id": "b5e5b78d-45ed-454a-9d36-4505fbfdf78a",
                            "itemBody": "C. are more suitable for describing asset returns than asset prices.",
                            "position": 3
                        }
                    ]
                },
                "scoringAlgorithm": "Equivalence",
                "interactionTypeSlug": "choice"
            },
            "status": "mutable",
