        {
            "id": "53123",
            "entry": {
                "title": "CFA2404-R-s02-Q7",
                "feedback": {
                    "neutral": "\n            \u003cp style=\"margin-top: 0.75rem; margin-bottom: 0.75rem;\"\u003eC is correct. A lognormal distributed variable has a lower bound of zero. The lognormal distribution is also right skewed, which is a useful property in describing asset prices.\u003c/p\u003e\n         "
                },
                "itemBody": "\u003cp style=\"margin-top: 0.75rem; margin-bottom: 0.75rem;\"\u003eThe lognormal distribution is a more accurate model for the distribution of stock prices than the normal distribution because stock prices are:\u003c/p\u003e",
                "properties": {
                    "shuffleRules": {
                        "choices": {
                            "toLock": [],
                            "shuffled": false
                        }
                    },
                    "varyPointsByAnswer": false
                },
                "scoringData": {
                    "value": "b933812a-07a7-4eac-8b92-7323fcfefbad"
                },
                "answerFeedback": {},
                "calculatorType": "none",
                "interactionData": {
                    "choices": [
                        {
                            "id": "b303e24c-4986-4f06-b1e3-5e00db5ad7d9",
                            "itemBody": "A. symmetrical.",
                            "position": 1
                        },
                        {
                            "id": "269e5531-4d55-4b7e-b562-094b423663f2",
                            "itemBody": "B. unbounded.",
                            "position": 2
                        },
                        {
                            "id": "b933812a-07a7-4eac-8b92-7323fcfefbad",
                            "itemBody": "C. non-negative.",
                            "position": 3
                        }
                    ]
                },
                "scoringAlgorithm": "Equivalence",
                "interactionTypeSlug": "choice"
            },
            "status": "mutable",
