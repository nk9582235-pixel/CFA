    "items": [
        {
            "id": "55108",
            "entry": {
                "title": "ITEM_CFA-L1-2020A-FI-Pirie-11",
                "feedback": {},
                "itemBody": "\u003cp\u003eIn a securitization, the purchase agreement between the seller of the collateral and the special purpose entity \u003ci\u003emost likely\u0026nbsp;\u003c/i\u003eprovides:\u003c/p\u003e",
                "properties": {
                    "shuffleRules": {
                        "choices": {
                            "toLock": [],
                            "shuffled": false
                        }
                    },
                    "varyPointsByAnswer": false
                },
                "scoringData": {
                    "value": "e433b6b3-4b6d-4964-b887-d86fd9e3ab43"
                },
                "answerFeedback": {
                    "4aaeaaee-fc1f-4dae-8efe-0ea9bfbd9cef": "\u003cp\u003eIncorrect because\u0026nbsp;this is part of the prospectus. Another important legal document is the prospectus, which describes the structure of the securitization.\u003c/p\u003e",
                    "e433b6b3-4b6d-4964-b887-d86fd9e3ab43": "\u003cp\u003eCorrect because an important legal document is the purchase agreement between the seller of the collateral and the SPE, which\u0026nbsp;sets forth the representations and warranties that the seller makes about the assets sold. These representations and warranties assure investors about the quality of the assets.\u003c/p\u003e",
                    "f82f56af-6fe8-46ca-9fbd-b7dba296731d": "\u003cp\u003eIncorrect because\u0026nbsp;this is part of the prospectus. Securitizations often use several forms of\u003cbr\u003ecredit enhancements, which are documented in the prospectus.\u003c/p\u003e"
                },
                "calculatorType": "none",
                "interactionData": {
                    "choices": [
                        {
                            "id": "4aaeaaee-fc1f-4dae-8efe-0ea9bfbd9cef",
                            "itemBody": "\u003cp\u003eA. a description of the transaction structure.\u003c/p\u003e",
                            "position": 1
                        },
                        {
                            "id": "e433b6b3-4b6d-4964-b887-d86fd9e3ab43",
                            "itemBody": "\u003cp\u003eB. representations about the quality of the assets.\u003c/p\u003e",
                            "position": 2
                        },
                        {
                            "id": "f82f56af-6fe8-46ca-9fbd-b7dba296731d",
                            "itemBody": "\u003cp\u003eC. documentation of enhancements used to reduce credit risk.\u003c/p\u003e",
                            "position": 3
                        }
                    ]
                },
                "scoringAlgorithm": "Equivalence",
                "interactionTypeSlug": "choice"
            },
            "status": "mutable",
            "archived": false,
            "position": 1,
            "entryType": "Item",
